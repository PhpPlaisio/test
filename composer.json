{
  "name": "plaisio/test",
  "description": "PhpPlaisio: Test",
  "keywords": [
    "PhpPlaisio",
    "Test"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "ext-libxml": "*",
    "plaisio/cookie": "^2.1.0",
    "plaisio/error-logger-core": "^1.6.0",
    "plaisio/helper-ob": "^1.2.0",
    "plaisio/helper-url": "^1.2.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/request-handler": "^2.1.0",
    "plaisio/response": "^2.1.0",
    "plaisio/response-core": "^2.7.0",
    "setbased/php-stratum": "^6.2.0",
    "setbased/php-stratum-middle": "^5.12.0",
    "setbased/php-stratum-sqlite-pdo": "^2.0.0",
    "symfony/console": "^6.0|^7.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.0",
    "plaisio/console-kernel": "^1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Test\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
